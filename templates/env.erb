LANG=en_US.UTF-8
<% attributes = node.keys.select { |key| node[key].is_a?(String) && key != 'uptime' } %>
<% attributes.each do |attribute| %>
<%= attribute.upcase %>=<%= node[attribute] %>
<% end %>
<% node['vsts_agent']['additional_environment'].each do |variable, setting| %>
<%= variable %>=<%= setting %>
<% end %>
